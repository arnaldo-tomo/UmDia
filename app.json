      "image": "./assets/spa.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cloud.png",
        "backgroundColor": "#1F212C"
      },
      "package": "com.arnaldotomo.powerGreen"
    },
    "web": {
      "favicon": "./assets/spa.png"
    },
    "extra": {
      "eas": {
        "projectId": "08884418-7a13-459b-a571-e4a4def4c5e7"
      }
    }
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "production": {}
  }
}
